<h1>User</h1>

<table>
  <thead>
    <tr>
      <th>image</th>
      <th>name</th>
    </tr>
  </thead>

  <tbody>
    
      <tr>
        <td>
        <%= image_tag @user.get_profile_image(100,100) %>
        </td>
        <% @users.each do |user| %>
        <td><%= user.name %></td>
          <td><%= link_to 'Show', user_path(user.id) %></td>
        </tr>
        <% end %>
  </tbody>
</table>